        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Select"
            ]
        },
        "runtime_list": {
            "parent": "left_pane",
            "area": [ 5, 5, -5, -5 ],
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 25,
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
