{
    "#info": {
        "version": 1,
        "name": "Spruce Portmaster",
        "creator": "tenlevels",
        "description": "A Portmaster theme for Spruce. Inspired by the default theme and Hi Viz theme by Johnny on Flame and Tekkenfede. Thanks to the creators of these themes for their inspiration."
    },
    "#pallet": {
        "text": "#EBDBB2",
        "text-select": "#EBDBB2",
        "text-no-select": "#928374",
        "background": "#282828",
        "progress-empty": "#3c3836",
        "progress-fill": "#689D6A",
        "selection-bg": "#504945",
        "list-header": [214, 93, 14]
    },
    "#schemes": {
        "#base": {
            "#resources": {
                "buttons.png": {
                    "atlas": {
                        "_A": [0, 0, 180, 180],
                        "_B": [180, 0, 180, 180],
                        "_X": [360, 0, 180, 180],
                        "_Y": [540, 0, 180, 180],
                        "_START": [0, 180, 360, 180],
                        "_SELECT": [360, 180, 360, 180],
                        "_L": [0, 360, 270, 180],
                        "_R": [270, 360, 270, 180],
                        "_UNCHECKED": [720, 0, 180, 180],
                        "_CHECKED": [900, 0, 180, 180],
                        "_MORE": [720, 180, 180, 180],
                        "_LEFT": [900, 180, 90, 180],
                        "_RIGHT": [990, 180, 90, 180],
                        "_UP": [900, 360, 180, 90],
                        "_DOWN": [900, 450, 180, 90]
                    }
                },
                "buttons_2.png": {
                    "atlas": {
                        "_UNCHECKED": [720, 0, 180, 180],
                        "_CHECKED": [900, 0, 180, 180]
                    }
                },
                "no-image.png": {
                    "name": "NO_IMAGE"
                }
            }
        },
        "Gruvbox": {
            "#resources": {
                "buttons.png": {
                    "image-mod": [215, 180, 95]
                },
                "buttons_2.png": {
                    "image-mod": [104, 168, 89]
                }
            }
        }
    },
    "#base": {
        "font": "nunwen.ttf",
        "font-size": 20,
        "font-color": "text",
        "select-color": "text-select",
        "no-select-color": "text-no-select",
        "progress-fill": "progress-fill",
        "select-fill": "selection-bg",
        "font-scale[640x480]": 1.0,
        "font-scale[1024x768]": 1.6,
        "font-scale[1280x720]": 1.5,
        "font-scale[1280x800]": 1.5,
        "font-scale[hires,!960x544]": 1.5,
        "font-scale[1920x1152]": 2.2,
        "font-scale[1920x1080]": 2.1,
        "font-scale[3:2]": 0.7
    },
    "#override": {
        "_A":         "A:",
        "_B":         "B:",
        "_X":         "X:",
        "_Y":         "Y:",
        "_UP":        "UP:",
        "_DOWN":      "DOWN:",
        "_LEFT":      "LEFT:",
        "_RIGHT":     "RIGHT:",
        "_START":     "START:",
        "_SELECT":    "SELECT:",
        "_L":         "L",
        "_R":         "R",
        "_CHECKED":   "[x]",
        "_UNCHECKED": "[  ]"
    },
    "#elements": {
        "background": {
            "area": [ 0.0, 0.0, 1.0, 1.0 ],
            "fill": "background"
        },
        "main_area": {
            "area": [ 5, 5, -5, -35 ]
        },
        "left_pane": {
            "area": [ 0.0, 0.0, 0.4, 0.93 ]
        },
        "right_pane": {
            "area": [ 0.4, 0.0, 1.0, 0.93 ]
        },
        "port_info_image_area": {
            "parent": "right_pane",
            "area": [ 10, 10, -10, 0.5 ],
            "fill": "#1d2021",
            "roundness": 5
        },
        "port_info_text_area": {
            "parent": "right_pane",
            "area": [ 10, 0.5, -10, -10 ]
        },
        "button_bar": {
            "area": [0.0, 0.93, 0.95, -5],
            "font-size": 24,
            "font-size[1024x768]": 30,
            "font-size[1280x720]": 30,
            "font-size[1280x800]": 30,
            "font-color": "text",
            "align": "midright",
            "bar": [
                "_B",
                "Back",
                "_A",
                "Accept"
            ]
        }
    },
    "ports_list": {
        "#element:background": {},
        "ports_list": {
            "parent": "left_pane",
            "area": [ 5, 5, -5, -5 ],
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 25,
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500,
            "list-width": "fit"
        },
        "port_info_image": {
            "parent": "port_info_image_area",
            "area": [ 0, 0, 1.0, 1.0 ],
            "image": "{port_info.image}",
            "fill": "#1d2021",
            "outline": "#504945",
            "thickness": 2,
            "roundness": 5,
            "smooth": true
        },
        "port_info": {
            "parent": "port_info_text_area",
            "area": [ 0, 0, 1.0, 1.0 ],
            "text": "Title: {port_info.title}\nPorter: {port_info.porter}\nGenres: {port_info.genres}\nDownload Size: {port_info.download_size}{if:port_info.runtime}\nRuntime: {port_info.runtime} ({port_info.runtime_status}){endif}\nDescription: {port_info.description}",
            "text-wrap": true,
            "font-size": 20,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Select"
            ]
        }
    },
    "port_info": {
        "#element:background": {},
        "port_info": {
            "parent": "main_area",
            "area": [ 0.0, 0.0, 1.0, 0.93 ],
            "text": "Title: {port_info.title}\nPorter: {port_info.porter}\nGenres: {port_info.genres}\nDownload Size: {port_info.download_size}{if:port_info.runtime}\nRuntime: {port_info.runtime} ({port_info.runtime_status}){endif}\nDescription: {port_info.description}",
            "text-wrap": true,
            "font-size": 20,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Accept"
            ]
        }
    },
    "port_info_popup": {
        "#element:background": {},
        "port_info_image": {
            "area": [ 0.1, 0.1, 0.9, 0.7 ],
            "image": "{port_info.image}",
            "fill": "#1d2021",
            "outline": "#504945",
            "thickness": 2,
            "roundness": 5,
            "smooth": true
        },
        "port_info_text": {
            "area": [ 0.1, 0.7, 0.9, 0.9 ],
            "text": "{port_info.title}",
            "text-wrap": true,
            "font-size": 20,
            "align": "center",
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Continue"
            ]
        }
    },
    "main_menu": {
        "#element:background": {},
        "option_list": {
            "parent": "main_area",
            "area": [ 0.1, 0.25, 0.9, 0.93 ],
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 28,
            "align": "midleft",
            "list-spacer": 12,
            "text-indent": 20,
            "pad-x": 10,
            "pad-y": 8
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Quit",
                "_A",
                "Select"
            ]
        }
    },
    "option_menu": {
        "#element:background": {},
        "option_list": {
            "parent": "main_area",
            "area": [ 0.0, 0.0, 1.0, 0.93 ],
            "area[640x480]": [ 5, 5, -5, -5 ],
            "area[1024x768]": [ 8, 8, -8, -8 ],
            "area[1280x720]": [ 10, 10, -10, -10 ],
            "area[1280x800]": [ 10, 10, -10, -10 ],
            "area[hires,!960x544]": [ 0.0, 16, 1.0, -16 ],
            "area[1920x1152]": [ 0.0, 32, 1.0, -32 ],
            "area[1920x1080]": [ 0.0, 32, 1.0, -32 ],
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 25,
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500,
            "list-header-color": "list-header",
            "no-select-color": "list-header",
            "list-width": "fit"
        },
        "pm_version": {
            "area": [ 0.02, 0.95, 0.5, 0.99 ],
            "text": "PM: {system.portmaster_version}",
            "font-size": 18,
            "align": "bottomleft",
            "font-color": "#928374"
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Select"
            ]
        }
    },
    "filter_list": {
        "#element:background": {},
        "filter_list": {
            "parent": "main_area",
            "area": [ 0.0, 0.0, 1.0, 0.93 ],
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 23,
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500,
            "check-image": "buttons_2.png",
            "list-width": "fit"
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Back",
                "_A",
                "Select"
            ]
        }
    },
    "message_window": {
        "#element:background": {},
        "message_text": {
            "parent": "main_area",
            "area": [ 0.0, 0.0, -45, 0.93 ],
            "text": " ",
            "text-wrap": true,
            "align": "bottomleft",
            "font-size": 20,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500
        },
        "progress_bar": {
            "area": [ 0.0, -20, 1.0, 0.93 ],
            "parent": "main_area",
            "fill": "progress-empty",
            "bar": ["{system.progress_text}", null, "{system.progress_amount}"],
            "align": "midleft"
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "Cancel",
                "_A",
                "OK"
            ]
        }
    },
    "message_box": {
        "#element:background": {},
        "message_text": {
            "parent": "main_area",
            "area": [ 0.15, 0.25, 0.85, 0.75 ],
            "area[1024x768]": [ 0.2, 0.2, 0.8, 0.8 ],
            "area[1280x720]": [ 0.2, 0.2, 0.8, 0.8 ],
            "area[1280x800]": [ 0.2, 0.2, 0.8, 0.8 ],
            "text": "",
            "text-wrap": true,
            "align": "center",
            "font-size": 26,
            "font-size[1024x768]": 32,
            "font-size[1280x720]": 32,
            "font-size[1280x800]": 32,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500
        },
        "#element:button_bar": {
            "bar": [
                "_B",
                "No",
                "_A",
                "Yes"
            ]
        }
    },
    "themes_list": {
        "#element:background": {},
        "themes_list": {
            "parent": "left_pane",
            "area": [ 5, 5, -5, -5 ],
            "area[640x480]": [ 5, 5, -5, -5 ],
            "area[1024x768]": [ 8, 8, -8, -8 ],
            "area[1280x720]": [ 10, 10, -10, -10 ],
            "area[1280x800]": [ 10, 10, -10, -10 ],
            "area[hires,!960x544]": [ 0.0, 16, 1.0, -16 ],
            "area[1920x1152]": [ 0.0, 32, 1.0, -32 ],
            "area[1920x1080]": [ 0.0, 32, 1.0, -32 ],
            "border-x": 8,
            "border-x[1024x768]": 12,
            "border-x[1280x720]": 12,
            "border-x[1280x800]": 12,
            "border-x[hires,!960x544]": 16,
            "border-x[1920x1152]": 32,
            "border-x[1920x1080]": 32,
            "list": [],
            "select-fill": "selection-bg",
            "roundness": 3,
            "font-size": 25,
            "text-clip": true,
            "text-wrap": false,
            "autoscroll": "slide",
            "scroll-speed": 30,
            "scroll-delay-start": 1500,
            "scroll-delay-end": 1500,
            "list-header-color": "list-header",
            "no-select-color": "list-header",
            "list-width": "fit"
        },
        "theme_image": {
            "parent": "port_info_image_area",
            "area": [ 0, 0, 1.0, 1.0 ],
            "area[hires,!960x544]": [ 0, 16, 1.0, 1.0 ],
            "area[1920x1152]": [ 0, 32, 1.0, 1.0 ],
            "area[1920x1080]": [ 0, 32, 1.0, 1.0 ],
            "image": "{theme_info.image}",
            "fill": "#1d2021",
            "outline": "#504945",
            "thickness": 2,
            "roundness": 5,
            "smooth": true
        },
        "theme_general_info": {
            "parent": "port_info_text_area",
            "area": [ 8, 8, -8, -48 ],
            "area[3:2]": [ 8, 8, -8, -36 ],
            "area[hires,!960x544]": [ 16, 16, -16, -64 ],
            "area[1920x1152]": [ 32, 32, -32, -116 ],
            "area[1920x1080]": [ 32, 32, -32, -116 ],
