#: frontend/ui/data/creoptions.lua:321
msgid "legacy"
msgstr "застарели"

#: frontend/ui/data/creoptions.lua:321
msgid "flat"
msgstr "равни"

#: frontend/ui/data/creoptions.lua:321
msgid "book"
